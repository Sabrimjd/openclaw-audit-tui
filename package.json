{
  "name": "openclaw-audit-tui",
  "version": "0.1.4",
  "description": "OpenClaw audit terminal UI built with OpenTUI",
  "module": "src/index.tsx",
  "type": "module",
  "private": false,
  "bin": {
    "openclaw-audit-tui": "bin/openclaw-audit-tui"
  },
  "scripts": {
    "dev": "bun run --watch src/index.tsx",
    "start": "bun run src/index.tsx",
    "typecheck": "bunx tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sabrimjd/openclaw-audit-tui.git"
  },
  "keywords": [
    "openclaw",
    "audit",
    "tui",
    "opentui",
    "terminal-ui"
  ],
  "author": "Sabrimjd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sabrimjd/openclaw-audit-tui/issues"
  },
  "homepage": "https://github.com/Sabrimjd/openclaw-audit-tui#readme",
  "engines": {
    "bun": ">=1.1.0"
  },
  "files": [
    "bin",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@opentui/core": "^0.1.81",
    "@opentui/react": "^0.1.81",
    "react": "^19.2.4"
  }
}
